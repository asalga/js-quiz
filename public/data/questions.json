        {
            "id": 39,
            "name": "comparing primitives",
            "q": [
            {
                "note": "Which of the following evaluates to true?",
                "code": [
                    "[] == 0",
                    "{} == {}",
                    "[] == false",
                    "''*'' === 0",
                    "''**'' === 0",
                    "`` == ``",
                    "0 == 'false'",
                    "NaN == NaN",
                    "'str' === 'str'",
                    "0/0 == Infinity",
                    "0 <= null",
                    "String('x') === new String('x')",
                    "0.1 + 0.2 === 0.3",
                    "!!'false' === !!'true'"
                ]
            }],
            "a": [
            {
                "note": "Results:",
                "code": [
                    "[] == 0           // true",
                    "{} == {}          // false",
                    "[] == false       // true",
                    "''*'' === 0       // true",
                    "''**'' === 0      // false",
                    "`` == ``          // true",
                    "0 == 'false'      // false",
                    "NaN == NaN        // false",
                    "0/0 == Infinity   // false 0/0 is NaN",
                    "0 <= null         // true",
                    "String('x') === new String('x')  // false",
                    "0.1 + 0.2 === 0.3 // false"
                ]
            }],
            "tags": "variable",
            "complexity": "8",
            "references": [],
            "notes": "",
            "see-also": "",
            "status": "published"
        },

